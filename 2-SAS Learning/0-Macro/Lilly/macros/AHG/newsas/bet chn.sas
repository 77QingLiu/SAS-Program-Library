data score sasuser.score;
  infile datalines truncover;
  format line  $100.;
  input line 1-100;
  if index(line,'星期') then date+1;
  retain date i id win tie lose win2 tie2 lose2 0;
  if substr(line,1,1)='0' and substr(line,4,1)=' ' and substr(line,6,1)>' '  then i=1;  else i=i+1;
  if i=1 then id=input(scan(line,1),best.);
  if i=1 then put test= id=;
  if i=5 then given=input(line,best.);
  if i=7 then  do;win=substr(line,1,4);tie=substr(line,5,4);lose=substr(line,9,4); end;
  if i=8 then  do;win2=substr(line,1,4);tie2=substr(line,5,4);lose2=substr(line,9,4); end;
  keep date id win tie lose win2 tie2 lose2 ;
  if i=8 then output;
cards;













